    "dependencies": {
        "@google/genai": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "nodemon": "^3.1.10"
    },
    "devDependencies": {
        "prettier": "3.5.3"
    }
}
